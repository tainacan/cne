{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	
	"version": 2,

	"settings": {

		"appearanceTools": true,

		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"em",
				"rem",
